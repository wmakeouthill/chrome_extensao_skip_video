{
  "name": "auto-skip-video-website",
  "version": "1.0.0",
  "description": "Website para Auto Skip Video Extension",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --base-href /chrome_extensao_skip_video/",
    "build:gh": "ng build --configuration production --base-href /chrome_extensao_skip_video/",
    "watch": "ng build --watch --configuration development",
